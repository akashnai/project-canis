{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "src/typeses"]
  },
  "include": ["src", "src/commands/..ts", "..ts"]
}
